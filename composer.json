{
    "name": "spatie/laravel-mailcoach-postmark-feedback",
    "description": "Process feedback for email campaigns sent using Postmark",
    "keywords": [
        "spatie",
        "laravel-mailcoach-postmark-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Benjamin Eberlei",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
    {
        "type": "composer",
        "url": "https://satis.mailcoach.app"
    }
],
    "require": {
        "php": "^7.4",
        "illuminate/http": "^7.0",
        "spatie/laravel-mailcoach": "^2.1",
        "spatie/laravel-webhook-client": "^2.2"
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachPostmarkFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachPostmarkFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachPostmarkFeedback\\MailcoachPostmarkFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
